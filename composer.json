{
  "name": "kodansha/wack-preview",
  "description": "Helpers to make it possible to preview posts on frontend.",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KODANSHA Ltd.",
      "homepage": "https://github.com/kodansha"
    },
    {
      "name": "Hirofumi Wakasugi",
      "homepage": "https://github.com/5t111111"
    }
  ],
  "homepage": "https://github.com/kodansha/wack-preview",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=8.2",
    "firebase/php-jwt": "^7.0"
  },
  "require-dev": {
    "10up/wp_mock": "^1.0",
    "laravel/pint": "^1.0"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "test": "phpunit",
    "lint": "pint --test",
    "lint:fix": "pint"
  },
  "autoload": {
    "psr-4": {
      "WackPreview\\": "src"
    }
  }
}
